msgctxt "@info"
msgid "No text matching the filter"
msgstr "Nincs a szűrőre illeszkedő szöveg"

#: qml/CommandOutputKCM.qml:93
#, kde-format
msgctxt "@info the KCM has no data to display"
msgid "No data available"
msgstr "Nincs elérhető adat"

#: qml/CommandOutputKCM.qml:106
#, kde-format
msgid "Report this issue"
msgstr "Jelentse ezt a hibát"

#: qml/CommandOutputKCM.qml:127
#, kde-format
