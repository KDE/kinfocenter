#: CommandOutputContext.cpp:121 CommandOutputContext.cpp:138
#, kde-kuit-format
msgctxt "@info"
msgid "Could not load page content"
msgstr "No s'ha pogut carregar el contingut de la pàgina"

#: CommandOutputContext.cpp:123
#, kde-kuit-format
msgctxt "@info"
msgid ""
"Technical details: The <command>%1</command> tool is required to display "
"this page, but could not be found. You may be able to install it using your "
"package manager; either way, please report this packaging issue to %2."
msgstr ""
"Detalls tècnics: cal l'eina <command>%1</command> per a mostrar aquesta "
"pàgina, però no s'ha pogut trobar. Podríeu ser capaç d'instal·lar-la fent "
"servir el gestor de paquets; en cas contrari, informeu d'aquesta incidència "
"d'empaquetament a %2."

#: CommandOutputContext.cpp:139
#, kde-kuit-format
msgctxt "@Info"
msgid ""
"Technical details: the <command>%1</command> tool crashed while generating "
"page content."
msgstr ""
"Detalls tècnics: l'eina <command>%1</command> ha fallat mentre generava el "
"contingut de la pàgina."

#: qml/CommandOutputKCM.qml:89
#, kde-format
msgctxt "@info"
msgid "No text matching the filter"
msgstr "Cap text coincideix amb el filtre"

#: qml/CommandOutputKCM.qml:94
#, kde-format
msgctxt "@info the KCM has no data to display"
msgid "No data available"
msgstr "No hi ha dades disponibles"

#: qml/CommandOutputKCM.qml:107
#, kde-format
msgid "Report to %1"
msgstr "Informa a %1"

#: qml/CommandOutputKCM.qml:128
#, kde-format
msgctxt "@action:button copies all displayed text"
msgid "Copy to Clipboard"
msgstr "Copia al porta-retalls"

#: qml/CommandOutputKCM.qml:139
#, kde-format
msgctxt "@label placeholder text to filter for something"
msgid "Filter…"
msgstr "Filtre…"

#: qml/CommandOutputKCM.qml:141
#, kde-format
msgctxt "accessible name for filter input"
msgid "Filter"
msgstr "Filtre"

