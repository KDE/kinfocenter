#: CommandOutputContext.cpp:127 CommandOutputContext.cpp:149
#, kde-kuit-format
msgctxt "@info"
msgid "Could not load page content"
msgstr "Non poteva cargar contento de pagina"

#: CommandOutputContext.cpp:129
#, kde-kuit-format
msgctxt "@info"
msgid ""
"Technical details: The <command>%1</command> tool is required to display "
"this page, but could not be found. You may be able to install it using your "
"package manager; either way, please report this packaging issue to %2."
msgstr ""
"Detalios Technic : Le instrumento  <command>%1</command> es requirite per "
"monstrar iste pagina, ma nonpoteva esser trovate. Tu poterea installar lo "
"per usar tu gerente de pacchettos; alteremente, pro favor reporta iste "
"problema de pacchettos a %2."

#: CommandOutputContext.cpp:150
#, kde-kuit-format
msgctxt "@Info"
msgid ""
"Technical details: the <command>%1</command> tool crashed while generating "
"page content."
msgstr ""
"Detalios technic: Le instrumento  <command>%1</command> fracassava durante "
"le generation de contento de pagina"

